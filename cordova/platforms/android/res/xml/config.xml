<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.pfn.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <preference name="webView" value="org.crosswalk.engine.XWalkWebViewEngine" />
    <preference default="16+" name="xwalkVersion" />
    <preference default="--disable-pull-to-refresh-effect" name="xwalkCommandLine" />
    <preference default="embedded" name="xwalkMode" />
    <preference default="true" name="xwalkMultipleApk" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="platforms/android/res/land-hdpi/screen.png" />
    <splash density="land-ldpi" src="platforms/android/res/land-ldpi/screen.png" />
    <splash density="land-mdpi" src="platforms/android/res/land-mdpi/screen.png" />
    <splash density="land-xhdpi" src="platforms/android/res/land-xhdpi/screen.png" />
    <splash density="land-xxhdpi" src="platforms/android/res/land-xxhdpi/screen.png" />
    <splash density="land-xxxhdpi" src="platforms/android/res/land-xxxhdpi/screen.png" />
    <splash density="port-hdpi" src="platforms/android/res/port-hdpi/screen.png" />
    <splash density="port-ldpi" src="platforms/android/res/port-ldpi/screen.png" />
    <splash density="port-mdpi" src="platforms/android/res/port-mdpi/screen.png" />
    <splash density="port-xhdpi" src="platforms/android/res/port-xhdpi/screen.png" />
    <name>Pfn</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://192.168.1.16:4200/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="xwalkVersion" value="16+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
